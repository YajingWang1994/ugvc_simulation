    <!-- Base link is the center of the robot's bottom plate -->
    <link name="base_link">
      <visual>
        <origin xyz="0 0 0.05" rpy="0 0 0" />
        <geometry>
          <box size="${length_x} ${length_y} ${length_z}"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0 0 ${length_z}" rpy="0 0 0" />
        <geometry>
          <box size="${length_x} ${length_y} ${length_z}"/>
        </geometry>
      </collision>
      <visual>
        <origin xyz="-0.08 0 0" rpy="0 0 1.5707" />
        <geometry>
          <mesh filename="package://husky_description/meshes/crawler.dae" />
        </geometry>
      </visual>
